//PROGRAM TO PRINT LEAST NUMBER AMONG THE NUMBERS//
#include<stdio.h>
main()
{
	int n,num,i,h=0;
	printf("Enter the number of numbers you want:");
	scanf("%d",&n);
	i=1;
	while(i<=n)
	{
		scanf("%d",&num);
		if(i==1)
		h=num;
		if(num<h)
		h=num;
		i++;
	}
		printf("Least among %d numbers:",n);
		printf("%d",h);
		return 0;
}
